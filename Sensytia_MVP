        "import ipywidgets as widgets\n",
        "from IPython.display import display\n",
        "\n",
        "# This function updates the list whenever you move the slider\n",
        "def filter_high_risk(threshold):\n",
        "    # 1. Filter the data based on the slider value\n",
        "    flagged_txns = df[df['Risk_Score'] > threshold]\n",
        "\n",
        "    # 2. Count how much money is at risk\n",
        "    risk_amount = flagged_txns['Amount'].sum()\n",
        "\n",
        "    print(f\"--- SENSYTIA LIVE DASHBOARD ---\")\n",
        "    print(f\"Showing transactions with Risk Score > {threshold}\")\n",
        "    print(f\"Total 'At Risk' Capital: ${risk_amount:,.2f}\")\n",
        "    print(f\"Transactions Found: {len(flagged_txns)}\")\n",
        "    print(\"\\n--- Details ---\")\n",
        "\n",
        "    # 3. Show the actual rows\n",
        "    if not flagged_txns.empty:\n",
        "        display(flagged_txns[['Date', 'Category', 'Amount', 'Risk_Score']].head(10))\n",
        "    else:\n",
        "        print(\"No transactions found at this risk level (Good news!)\")\n",
        "\n",
        "# Create the slider\n",
        "widgets.interact(filter_high_risk, threshold=widgets.IntSlider(min=0, max=100, step=5, value=80));"
