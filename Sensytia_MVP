        "from google.colab import files\n",
        "\n",
        "# 1. Define the Export Function\n",
        "def export_high_risk_report():\n",
        "    # Filter for the high risk items (Score > 80)\n",
        "    report_df = df[df['Risk_Score'] > 80]\n",
        "\n",
        "    # Check if there is anything to download\n",
        "    if not report_df.empty:\n",
        "        filename = 'Sensytia_High_Risk_Report.csv'\n",
        "        report_df.to_csv(filename, index=False)\n",
        "        files.download(filename) # This triggers the browser download\n",
        "        print(f\" Success! Downloaded {len(report_df)} flagged transactions.\")\n",
        "    else:\n",
        "        print(\" No high-risk transactions found to export.\")\n",
        "\n",
        "# 2. Run the export\n",
        "# In a web app, this would be a button click.\n",
        "# Here, running the cell acts as the button.\n",
        "export_high_risk_report()"
      ],
