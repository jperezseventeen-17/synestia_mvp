      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "import random\n",
        "\n",
        "# 1. Setup: How many fake transactions do you want?\n",
        "num_rows = 50\n",
        "\n",
        "# 2. Generate random data\n",
        "data = {\n",
        "    'Date': pd.date_range(start='2025-01-01', periods=num_rows),\n",
        "    'Transaction_ID': [f'TXN{x}' for x in range(1000, 1000 + num_rows)],\n",
        "    'Category': np.random.choice(['Software', 'Travel', 'Meals', 'Server Costs'], num_rows),\n",
        "    'Amount': np.random.uniform(5.00, 1500.00, num_rows).round(2), # Random amounts between $5 and $1500\n",
        "    'Risk_Score': np.random.randint(1, 100, num_rows) # A random \"risk\" score for fintech analysis\n",
        "}\n",
        "\n",
        "# 3. Create the DataFrame (The Table)\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "# 4. Preview the first 5 rows\n",
        "print(f\"--- Generated {num_rows} Rows of Mock Data for Sensytia ---\")\n",
        "print(df.head())\n",
        "\n",
        "# 5. Quick Test: Find the \"High Risk\" transactions (Score > 80)\n",
        "high_risk = df[df['Risk_Score'] > 80]\n",
