    {
      "cell_type": "code",
      "source": [
        "# --- SENSYTIA ALGORITHM V1.0 ---\n",
        "\n",
        "def calculate_smart_risk(row):\n",
        "    score = 10 # Base risk is low\n",
        "\n",
        "    # Rule 1: Big Spenders (Any transaction over $800 is risky)\n",
        "    if row['Amount'] > 800:\n",
        "        score += 50\n",
        "\n",
        "    # Rule 2: Suspicious Categories (Server Costs shouldn't be high unless approved)\n",
        "    if row['Category'] == 'Server Costs' and row['Amount'] > 500:\n",
        "        score += 30\n",
        "\n",
        "    # Rule 3: The \"Coffee Test\" (Meals usually aren't over $200)\n",
        "    if row['Category'] == 'Meals' and row['Amount'] > 200:\n",
        "        score += 40\n",
        "\n",
        "    # Cap the score at 100\n",
        "    return min(score, 100)\n",
        "\n",
        "# Apply this \"Brain\" to your data\n",
        "df['Risk_Score'] = df.apply(calculate_smart_risk, axis=1)\n",
        "\n",
        "print(\"--- Logic Update Complete ---\")\n",
        "print(\"Sensytia has re-scored all transactions based on custom rules.\")\n",
        "print(df.sort_values(by='Risk_Score', ascending=False).head(5))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
